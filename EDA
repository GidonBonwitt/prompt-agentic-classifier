   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Assuming you've already defined:\n",
    "#   short_harmful          a DataFrame of your 200 shortest harmful prompts, with a 'length' column\n",
    "#   short_benign_matched   a DataFrame of matched benign prompts, also with a 'length' column\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(12, 5), sharey=True)\n",
    "\n",
    "# Plot for short_harmful\n",
    "axes[0].hist(short_harmful[\"length\"], bins=50, edgecolor=\"black\")\n",
    "axes[0].set_title(\"Short Harmful Prompts Length Distribution\")\n",
    "axes[0].set_xlabel(\"Prompt length (chars)\")\n",
    "axes[0].set_ylabel(\"Count\")\n",
    "\n",
    "# Plot for short_benign_matched\n",
    "axes[1].hist(short_benign_matched[\"length\"], bins=50, edgecolor=\"black\")\n",
    "axes[1].set_title(\"Matched Short Benign Prompts Length Distribution\")\n",
    "axes[1].set_xlabel(\"Prompt length (chars)\")\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
