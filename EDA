   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Number of prompts in matched_short_benign_prompts.parquet: 200\n",
      "Number of prompts in short_harmful_prompts.parquet: 200\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x500 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Load the datasets\n",
    "benign_matched = pd.read_parquet('data/matched_short_benign_prompts.parquet')\n",
    "short_harmful = pd.read_parquet('data/short_harmful_prompts.parquet')\n",
    "\n",
    "# Compute and print counts\n",
    "print(f\"Number of prompts in matched_short_benign_prompts.parquet: {len(benign_matched)}\")\n",
    "print(f\"Number of prompts in short_harmful_prompts.parquet: {len(short_harmful)}\")\n",
    "\n",
    "# Add 'length' column if missing\n",
    "for df in (benign_matched, short_harmful):\n",
    "    if 'length' not in df.columns:\n",
    "        df['length'] = df['prompt'].str.len()\n",
    "\n",
    "# Plot side-by-side histograms\n",
    "fig, axes = plt.subplots(1, 2, figsize=(12, 5), sharey=True)\n",
    "\n",
    "axes[0].hist(short_harmful['length'], bins=50, edgecolor='black')\n",
    "axes[0].set_title('Short Harmful Prompts')\n",
    "axes[0].set_xlabel('Prompt length (chars)')\n",
    "axes[0].set_ylabel('Count')\n",
    "\n",
    "axes[1].hist(benign_matched['length'], bins=50, edgecolor='black')\n",
    "axes[1].set_title('Matched Short Benign Prompts')\n",
    "axes[1].set_xlabel('Prompt length (chars)')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
