 "cells": [
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Saved 200 shortest harmful prompts to: data/short_harmful_prompts.parquet\n",
      "Saved 200 matched benign prompts to: data/matched_short_benign_prompts.parquet\n"
     ]
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Ensure you have pyarrow installed for Parquet support:\n",
    "#    pip install pyarrow\n",
    "\n",
    "import os\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 1. File paths\n",
    "data_dir        = \"data\"\n",
    "harmful_in      = os.path.join(data_dir, \"harmful_prompts.parquet\")\n",
    "benign_in       = os.path.join(data_dir, \"benign_prompts.parquet\")\n",
    "harmful_out     = os.path.join(data_dir, \"short_harmful_prompts.parquet\")\n",
    "benign_matched  = os.path.join(data_dir, \"matched_short_benign_prompts.parquet\")\n",
    "\n",
    "# 2. Load datasets\n",
    "harmful_df = pd.read_parquet(harmful_in)\n",
    "benign_df  = pd.read_parquet(benign_in)\n",
    "\n",
    "# 3. Compute prompt lengths\n",
    "harmful_df[\"length\"] = harmful_df[\"prompt\"].str.len()\n",
    "benign_df[\"length\"]  = benign_df[\"prompt\"].str.len()\n",
    "\n",
    "# 4. Take the 200 shortest harmful prompts\n",
    "n_harmful = 200\n",
    "short_harmful = harmful_df.nsmallest(n_harmful, \"length\").reset_index(drop=True)\n",
    "\n",
    "# 5. Bin the harmful lengths into 10 equal-width bins\n",
    "bins = np.linspace(short_harmful[\"length\"].min(),\n",
    "                   short_harmful[\"length\"].max(),\n",
    "                   num=11)\n",
    "short_harmful[\"bin\"] = pd.cut(short_harmful[\"length\"], bins=bins, include_lowest=True)\n",
    "\n",
    "# 6. Filter benign to the harmful range (remove outliers)\n",
    "benign_filtered = benign_df[\n",
    "    (benign_df[\"length\"] >= short_harmful[\"length\"].min()) &\n",
    "    (benign_df[\"length\"] <= short_harmful[\"length\"].max())\n",
    "].copy()\n",
    "\n",
    "# 7. Sample benign to match the harmful distribution\n",
    "matched_samples = []\n",
    "for interval, count in short_harmful[\"bin\"].value_counts().sort_index().items():\n",
    "    candidates = benign_filtered[\n",
    "        benign_filtered[\"length\"].between(interval.left, interval.right)\n",
    "    ]\n",
    "    replace = len(candidates) < count\n",
    "    sampled = candidates.sample(n=count, replace=replace, random_state=42)\n",
    "    matched_samples.append(sampled)\n",
    "short_benign_matched = pd.concat(matched_samples, ignore_index=True)\n",
    "short_benign_matched = short_benign_matched.sample(frac=1, random_state=42).reset_index(drop=True)\n",
    "\n",
    "# 8. Drop the bin column\n",
    "short_harmful = short_harmful.drop(columns=[\"bin\"])\n",
    "\n",
    "# 9. Save the new subsets\n",
    "short_harmful.to_parquet(harmful_out, index=False)\n",
    "short_benign_matched.to_parquet(benign_matched, index=False)\n",
    "\n",
    "print(f\"Saved {n_harmful} shortest harmful prompts to: {harmful_out}\")\n",
    "print(f\"Saved {len(short_benign_matched)} matched benign prompts to: {benign_matched}\")\n",
    "\n",
    "# 10. Display histograms (no saving)\n",
    "\n",
    "plt.figure(figsize=(12, 8))\n",
    "\n",
    "# Harmful: original vs shortest subset\n",
    "plt.subplot(2, 1, 1)\n",
    "plt.hist(harmful_df[\"length\"], bins=50, alpha=0.5, label=\"Original Harmful\")\n",
    "plt.hist(short_harmful[\"length\"], bins=bins, edgecolor=\"black\",\n",
    "         alpha=0.7, label=\"Shortest 200 Harmful\")\n",
    "plt.title(\"Harmful Prompts Length Distribution\")\n",
    "plt.xlabel(\"Prompt length (chars)\")\n",
    "plt.ylabel(\"Count\")\n",
    "plt.legend()\n",
    "\n",
    "# Benign: original filtered vs matched subset\n",
    "plt.subplot(2, 1, 2)\n",
    "plt.hist(benign_filtered[\"length\"], bins=50, alpha=0.5, label=\"Benign (Filtered Range)\")\n",
    "plt.hist(short_benign_matched[\"length\"], bins=bins, edgecolor=\"black\",\n",
    "         alpha=0.7, label=\"Matched 200 Benign\")\n",
    "plt.title(\"Benign Prompts Length Distribution\")\n",
    "plt.xlabel(\"Prompt length (chars)\")\n",
    "plt.ylabel(\"Count\")\n",
    "plt.legend()\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
  {
